<div id="content">
	<hr>
	<h3>Create a new ticket <span class="badge badge-warning">WIP</span></h3>
	<section class="mt-3 mb5">
	<form id="getFormForm" treetop action="/ticket/form">
		<div class="form-group row">
			<label for="projectSelect" class="col-sm-2 col-form-label">Project</label>
			<div class="col-sm-10">
				<select class="form-control" id="projectSelect" name="Project" onchange="treetop.submit(forms.getFormForm)" required="required">
					<option>Project 1</option>
					<option>Project 2</option>
					<option>Project 3</option>
					<option>Project 4</option>
					<option>Project 5</option>
				</select>
			</div>
		</div>
		<div class="form-group row">
			<label for="ticketTypeFormSelect" class="col-sm-2 col-form-label">Issue Type</label>
			<div class="col-sm-10">
				<select class="form-control" name="ticket-type" id="ticketTypeFormSelect" onchange="treetop.submit(forms.getFormForm)" required="required">
					<option>Task</option>
					<option>Bug</option>
					<option>Epic</option>
					<option>Wish List</option>
				</select>
			</div>
		</div>

		{{ template "form" .Form}}

	</form>
	</section>
</div>